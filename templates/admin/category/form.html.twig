{% extends 'admin.base.html.twig' %}

{% block body %}
    <div class="row">
        <div class="col">
            <h1>Formulaire des oeuvres</h1>
            {{ form_start(form, { attr : { novalidate: 'novalidate' } }) }}
            <p>
                {{ form_label(form.name, 'Nom: ') }}
                {{ form_widget(form.name) }}
            </p>
            <p>
                <input type="submit" value="Valider" class="btn btn-primary">
            </p>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}